{
  "name": "eslint-plugin-styles",
  "version": "1.0.1",
  "repository": "github:eslint-tools/eslint-plugin-styles",
  "main": "./src/index.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\"",
    "lint": "eslint --max-warnings 0 \"src/**/*.js\"",
    "test": "yarn format:check && yarn lint"
  },
  "dependencies": {},
  "devDependencies": {
    "eslint": "^7.29.0",
    "eslint-config-xt": "^1.8.0",
    "prettier": "^2.3.1",
    "typescript": "^4.3.4"
  },
  "eslintConfig": {
    "extends": "xt",
    "rules": {
      "@typescript-eslint/no-var-requires": 0
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "license": "ISC"
}
